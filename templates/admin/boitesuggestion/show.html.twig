{% extends 'AdminBase1.html.twig' %}


{% block title %}Boitesuggestion
{% endblock %}

{% block body %}
	<div class="container-fluid">
		<div class="d-sm-flex align-items-center justify-content-between mb-4">
			<h1 class="h3 mb-0 text-gray-800">Message de
				{{ boitesuggestion.nom }}</h1>


			<a href="{{ path('admin_boitesuggestion_index') }}" class="btn btn-info">Retour</a>


			{{ include('admin/boitesuggestion/_delete_form.html.twig') }}
		</div>
		<table class="table">
			<tbody>
				<tr>
					<th>Nom</th>
					<td>{{ boitesuggestion.nom }}</td>
				</tr>
				<tr>
					<th>Email</th>
					<td>{{ boitesuggestion.email }}</td>
				</tr>
				<tr>
					<th>Message</th>
					<td>{{ boitesuggestion.message }}</td>
				</tr>
			</tbody>
		</table>
	</div>
	<!-- /.container-fluid -->

{% endblock %}
