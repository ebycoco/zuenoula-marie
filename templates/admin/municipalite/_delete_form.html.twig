<form method="post" action="{{ path('admin_municipalite_delete', {'id': municipalite.id}) }}" onsubmit="return confirm('Etre vous sûr de vouloir supprimer le organe_de_fonctionnement numéro {{ municipalite.titre }} ?');">

	<input type="hidden" name="_token" value="{{ csrf_token('delete' ~ municipalite.id) }}">
	<button class="btn btn-danger btn-icon-split">
		<span class="icon text-white-50">
			<i class="fas fa-trash"></i>
		</span>
		<span class="text">Supprimer</span>
	</button>
</form>
